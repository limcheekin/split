{
  "name": "load-split-count",
  "description": "Load, Split and Count (LSC) endpoint",
  "version": "0.1.0",
  "main": "ingest.py",
  "keywords": [
    "load",
    "split",
    "count",
    "aws-lambda",
    "serverless-framework"
  ],
  "author": "Lim Chee Kin",
  "devDependencies": {
    "serverless-python-requirements": "^6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/limcheekin/load-split-count"
  },
  "bugs": {
    "url": "https://github.com/limcheekin/load-split-embed/issues"
  },
  "homepage": "https://github.com/limcheekin/load-split-embed#readme",
  "scripts": {
    "docker-build": "docker build -t document-splitter . --build-arg MODEL=Xenova/text-embedding-ada-002",
    "docker-save": "docker save document-splitter | gzip > document-splitter.tar.gz",
    "docker-load": "docker load -i document-splitter.tar.gz",
    "docker-run": "docker run -v /tmp:/tmp --env-file ./.env -p 8000:8000 -d document-splitter",
    "docker-stop": "./docker-stop.sh",
    "rm-all-docker-data": "docker system prune -a --volumes"
  }
}
